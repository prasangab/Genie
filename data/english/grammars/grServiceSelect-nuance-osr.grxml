            <item>1<tag>atag+='1'</tag>
            </item>
            <item>2<tag>atag+='2'</tag>
            </item>
            <item>3<tag>atag+='3'</tag>
            </item>
            <item>4<tag>atag+='4'</tag>
            </item>
            <item>5<tag>atag+='5'</tag>
            </item>
        </one-of>
    </rule>
</grammar>
