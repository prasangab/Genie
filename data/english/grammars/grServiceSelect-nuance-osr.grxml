<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<grammar xmlns="http://www.w3.org/2001/06/grammar" mode="dtmf" root="grServiceSelect" version="1.0" xml:lang="en-us">
    <rule id="grServiceSelect" scope="public">
